{
  "name": "universal",
  "version": "0.3.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:client": "webpack --config ./webpack.config.js/",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch --progress",
    "build:server": "babel ./src -d dist",
    "build:watch:server": "babel ./src -d dist --watch",
    "start:dev": "NODE_ENV='development' parallelshell 'npm run build:watch:server' 'npm run build:watch:client' 'nodemon ./start.js'",
    "build:prod": "npm run build:server && npm run build:client",
    "start": "npm run build:prod && NODE_ENV=production node ./start.js"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "express": "^4.16.3",
    "griddle-react": "^1.13.1",
    "ignore-styles": "^5.0.1",
    "morgan": "^1.9.0",
    "pg-promise": "^8.4.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.0",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "parallelshell": "^3.0.2",
    "postcss-loader": "2.0.6",
    "standard": "^11.0.1",
    "standard-loader": "^6.0.1",
    "style-loader": "0.18.2",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.7.1"
  },
  "standard": {
    "globals": [
      "it",
      "describe",
      "beforeEach",
      "afterEach",
      "before",
      "after",
      "Backbone",
      "_",
      "$",
      "csrf",
      "Storage",
      "sessionStorage",
      "pug",
      "fetch",
      "React",
      "__INITIAL_STATE__"
    ],
    "ignore": [
      "/src/components/CustomTableComponent.js"
    ]
  },
  "engines": {
    "node": ">=6"
  }
}